<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.GUI.mainwindow.MainWindowController">
   <children>
      <GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Label fx:id="progStatesLabel" text="Program States:" />
                  <ListView fx:id="progStatesListView" prefHeight="200.0" prefWidth="200.0" />
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1">
               <children>
                  <Label text="Heap table" />
                  <TableView fx:id="heapTableTableView" prefHeight="200.0" prefWidth="200.0">
                    <columns>
                      <TableColumn fx:id="heapAddressColumn" prefWidth="96.0" text="Adress" />
                      <TableColumn fx:id="heapValueColumn" prefWidth="203.0" text="Value" />
                    </columns>
                  </TableView>
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" GridPane.rowIndex="1">
               <children>
                  <Label text="Out" />
                  <ListView fx:id="outListView" prefHeight="200.0" prefWidth="200.0" />
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <children>
                  <Label text="File table" />
                  <ListView fx:id="fileTableListView" prefHeight="200.0" prefWidth="200.0" />
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" GridPane.rowIndex="2">
               <children>
                  <Label text="Symbol table" />
                  <TableView fx:id="symTableTableView" prefHeight="200.0" prefWidth="200.0">
                     <columns>
                        <TableColumn fx:id="symbolNameColumn" prefWidth="96.0" text="Name" />
                        <TableColumn fx:id="symbolValueColumn" prefWidth="203.0" text="Value" />
                     </columns>
                  </TableView>
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <children>
                  <Label alignment="TOP_LEFT" text="Execution stack" />
                  <ListView fx:id="executionStackListView" prefHeight="200.0" prefWidth="200.0" />
               </children>
            </VBox>
            <HBox alignment="CENTER" prefHeight="32.0" prefWidth="600.0" spacing="200.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
               <children>
                  <Button fx:id="stepButton" mnemonicParsing="false" text="Step" />
                  <Button fx:id="runButton" mnemonicParsing="false" text="Run" />
               </children>
            </HBox>
            <TableView fx:id="semaphoreTableView" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="3">
              <columns>
                <TableColumn fx:id="semaphoreTableIdColumn" prefWidth="42.0" text="Id" />
                <TableColumn fx:id="semaphoreTableCountColumn" prefWidth="91.0" text="Count" />
                  <TableColumn fx:id="semaphoreTableThreadsColumn" prefWidth="166.0" text="Threads" />
              </columns>
            </TableView>
         </children>
      </GridPane>
   </children>
</VBox>
